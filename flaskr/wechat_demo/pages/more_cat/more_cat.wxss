.words {
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: 600;
  font-size: larger;
  margin-bottom: 23rpx;
  margin-top: 23rpx;
  color: rgb(85, 84, 84);
}


.line{
  width:100%;
  height:2rpx; 
  background:#aaa;
}

.category {
  flex-direction:row;
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  margin-top: 15rpx;
  margin-bottom: 15rpx;
}

.icons {
  display: flex;
  flex-direction: row;
  justify-content: center; 
  align-items: center;
  background-color: white;
}

.icons image{
  width: 120rpx;
  height: 120rpx;
}

.icon_name {
  text-align: center;
}